import java.util.*;
 public class lfs
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
       int c=0;
        String s1="Tetrahedron";
        String s2="Cube";
        String s3="Octahedron";
        String s4="Dodecahedron";
        String s5="Icosahedron";
          
        while(n-->=0)
        {
        
             String s=sc.nextLine();
             if(s.equals(s1))
             {
                 c=c+4;
             }
            else if(s.equals(s2))
            {
                c=c+6;
            }
            else if(s.equals(s3))
            {
                c=c+8;
            }
             else if(s.equals(s4))
            {
                c=c+12;
            }
            else if(s.equals(s5))
            {
                c=c+20;
            }
        }
        System.out.println(c);
    }
 
}

import java.util.*;
public class lfs
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
    
        int k=sc.nextInt();
        for(int i=0;i<k;i++)
        {
            int j=n%10;
            if(j==0)
            {
                n=n/10;
               
            }
            else
            {
                n=n-1;
                
            }
            
        }
        System.out.println(n);
    }
}

import java.util.*;
public class lfs
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int t=sc.nextInt();
        while(t-->0)
        {
            int n=sc.nextInt();
            int f=0;
            for(int i=1;i<=9;i++)
            {
                int sum=0;
                for(int j=1;j<=4;j++)
                {
                    sum=sum*10+i;
                    f=f+j;
                    if(sum==n)
                    {
                        System.out.println(f);
                    }
                }
            }
        }
    }
}
                
                
            
            
        
    

import java.util.*;
 public class lfs
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int c=0;
        int sum=0;
        int temp=0;
        for(int i=1;;i++)
        {
            sum=sum+i;
            temp=temp+sum;
            if(temp>n)
            {
                break;
            }
            c++;
        }
        System.out.println(c);
    }
}

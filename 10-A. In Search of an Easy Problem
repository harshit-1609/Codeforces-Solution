import java.util.*;
public class lfs
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++)
        {
            a[i]=sc.nextInt();
            
        }
        boolean f=true;
        for(int j=0;j<a.length;j++)
        {
            if(a[j]==1)
            {
                f=false;
            }
        }
        if(f==true)
        {
            System.out.println("EASY");
        }
        else
        {
             System.out.println("HARD");
        }
        
    }
}

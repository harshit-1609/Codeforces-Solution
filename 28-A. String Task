import java.util.*;
public class lfs
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        String s=sc.nextLine();
        String q="";
        
        String p=s.replaceAll("[aeiouyAEIOUY]","");
          for(int i=0;i<p.length();i++)
        {
            q=q+"."+p.charAt(i);
        }
        q=q.toLowerCase();
    
        
        
        System.out.println(q);
        
    }
}

        
        
        
        
        
        
        
    

import java.util.*;
public class lfs
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int m=sc.nextInt();
        for(int i=1;i<=n;i++)
        {
            if((i%4)==2)
            {
                for(int j=1;j<m;j++)
                {
                    System.out.print(".");
                }
                System.out.print("#");
            }
            else if((i%4)==0)
            {
                System.out.print("#");
                for(int k=1;k<m;k++)
                {
                    System.out.print(".");
                }
            }
            else
            {
                for(int p=1;p<=m;p++)
                {
                    System.out.print("#");
                }
            }
            System.out.println();
        }
    }
}
